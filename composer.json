{
  "name": "greendot/eshopbundle",
  "description": "Eshop base for GD eshops",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Greendot",
      "email": "stepan@greendot.cz"
    }
  ],
  "autoload": {
    "psr-4": {
      "Greendot\\EshopBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Greendot\\EshopBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "symfony/dependency-injection": "^7.0",
    "doctrine/doctrine-bundle": "^2.12",
    "doctrine/orm": "^3.1",
    "symfony/serializer": "^7.0",
    "api-platform/core": "^3.2",
    "gedmo/doctrine-extensions": "*",
    "symfonycasts/reset-password-bundle": "^1.22",
    "symfony/form": "^7.1",
    "symfony/mailer": "^7.1",
    "symfony/mime": "^7.1",
    "symfony/messenger": "^7.1",
    "symfony/doctrine-messenger": "^7.1",
    "knplabs/knp-paginator-bundle": "^6.5",
    "symfony/security-bundle": "^7.1",
    "symfony/validator": "^7.1",
    "symfony/twig-bundle": "^7.1",
    "symfonycasts/verify-email-bundle": "^1.17",
    "symfony/config": "^7.1",
    "symfony/runtime": "^7.1",
    "symfony/yaml": "^7.1",
    "symfony/workflow": "^7.1",
    "stof/doctrine-extensions-bundle": "^1.12",
    "symfony/http-client": "^7.1",
    "symfony/dotenv": "^7.1"
  },
  "require-dev": {
    "symfony/http-kernel": "^7.1",
    "phpunit/phpunit": "^11.0",
    "doctrine/doctrine-fixtures-bundle": "^3.6",
    "zenstruck/foundry": "^2.2",
    "symfony/maker-bundle": "^1.61",
    "symfony/flex": "^2.4"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
